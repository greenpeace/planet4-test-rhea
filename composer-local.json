{
	"name": "greenpeace/planet4-test-rhea",
	"description": "Greenpeace P4 test-rhea",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-master",
		"plugins/sitepress-multilingual-cms": "*",
		"plugins/wpml-translation-management": "*",
		"plugins/wpml-string-translation": "*"
	},
	"scripts": {
		"site:custom": []
	},
	"data_from": "international",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/9a4741b7ecb3905ebe22c484ad1671e082547693.zip"
				}
			}
		}
	]
}
